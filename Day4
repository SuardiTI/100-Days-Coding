package Day4;

public class Main {
    
    public static void main(String[] args) {
        String nama = "Suardi";
        char jenis_kelamin = 'L';
        int umur = 18;
        String alamat = "Majene";
        char kelas = 'A';
        String jurusan = "Teknik Informatika";
        String nama_instansi  = "Universitas Sulawesi Barat";
        
        System.out.println("Nama : "+nama+"\n"+"Jenis Kelamin : "+jenis_kelamin+"\n"+"Umur : "+umur+"\n"+"Alamat : "+alamat+"\n"+"Kelas : "+kelas+"\n"+"Jurusan : "+jurusan+"\n"+"Nama Universitas : "+nama_instansi);
    }
    
}
